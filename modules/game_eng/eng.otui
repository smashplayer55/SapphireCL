MainWindow
  id: engWindow
  size: 550 400
  !text: tr('Engineer Workshop')
  padding: 2
  @onEnter: modules.game_eng.hide()
  @onEscape: modules.game_eng.hide()

  ClansButton
    text: gardestrike
    icon: imagens/estilista/todd
    margin-top: 25
    margin-left: 5

  UIButton
    id: closeButton
    anchors.top: parent.top
    anchors.right: parent.right
    margin-top: 2
    margin-right: 12
    size: 14 14
    image-source: /images/game/guide/button
    image-clip: 0 0 14 14
    @onClick: hide()

  UIButton
    id: aleatorio
    anchors.top: parent.top
    anchors.right: parent.right
    margin-top: 25
    margin-right: 15
    size: 330 370
    background-color: #000000AA

  VerticalScrollBar
    id: scrollBar
    step: 35
    pixels-scroll: true
    anchors.top: parent.top
    anchors.right: parent.right
    anchors.bottom: parent.bottom

  ScrollablePanel
    vertical-scrollbar: scrollBar
    anchors.fill: parent
    margin-right: 15    
    margin-top: 25

