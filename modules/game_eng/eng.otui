Teste < UIButton
  id: teste
  text-offset: 15 -10
  font: verdana-11px-rounded
  color: white
  icon-align: left
  icon-offset: 5 0
  height: 55
  width: 330
  background-color: #000000AA
  border-width: 1
  border-color: black
  anchors.top: parent.top
  anchors.right: parent.right

  $hover:
    background-color: #666666AA
    border-color: #666666

  $pressed:
    background-color: #333333AA
    border-color: #333333

  $disabled:
    background-color: #330000AA
    border-color: #330000
    color: red

